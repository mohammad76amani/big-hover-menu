                document.getElementById("fourmenusubmenu1").style.display = "block";
                // close other sub menues

            }
            if (document.getElementById("fourmenusubmenu1").style.display == "block") {
                document.getElementById("fourmenusubmenu2").style.display = "none";
                document.getElementById("fourmenusubmenu3").style.display = "none";
                document.getElementById("fourmenusubmenu4").style.display = "none";
                document.getElementById("fourmenusubmenu5").style.display = "none";
                document.getElementById("fourmenusubmenu6").style.display = "none";
            }
            //   open home  submenu 2 & close other submenues 
        }
        function menu4submenu2() {
            if (document.getElementById("fourmenusubmenu2").style.display == "none") { document.getElementById("fourmenusubmenu2").style.display = "block"; }

            if (document.getElementById("fourmenusubmenu2").style.display == "block") {
                document.getElementById("fourmenusubmenu1").style.display = "none";
                document.getElementById("fourmenusubmenu3").style.display = "none";
                document.getElementById("fourmenusubmenu4").style.display = "none";
                document.getElementById("fourmenusubmenu5").style.display = "none";
                document.getElementById("fourmenusubmenu6").style.display = "none";
            }
        }
