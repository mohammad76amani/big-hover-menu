 /////////////////////five menu script/////////////////////////////////
          // open five menu  and close other menues
          function menu5open() {
            if (document.getElementById("wearkit").style.display == "none") { 
                document.getElementById("wearkit").style.display = "block";
                document.getElementById("firstmenusubmenu1").style.display = "none";
                document.getElementById("firstmenusubmenu2").style.display = "none";
                document.getElementById("firstmenusubmenu3").style.display = "none";
                document.getElementById("firstmenusubmenu4").style.display = "none";
                document.getElementById("firstmenusubmenu5").style.display = "none";
                document.getElementById("firstmenusubmenu6").style.display = "none";
                document.getElementById("secondmenusubmenu1").style.display = "none";
                document.getElementById("secondmenusubmenu2").style.display = "none";
                document.getElementById("secondmenusubmenu3").style.display = "none";
                document.getElementById("secondmenusubmenu4").style.display = "none";
                document.getElementById("secondmenusubmenu5").style.display = "none";
                document.getElementById("secondmenusubmenu6").style.display = "none";
                document.getElementById("thirdmenusubmenu1").style.display = "none";
                document.getElementById("thirdmenusubmenu3").style.display = "none";
                document.getElementById("thirdmenusubmenu4").style.display = "none";
                document.getElementById("thirdmenusubmenu5").style.display = "none";
                document.getElementById("thirdmenusubmenu2").style.display = "none";
                document.getElementById("thirdmenusubmenu6").style.display = "none";
                document.getElementById("fourmenusubmenu1").style.display = "none";
                document.getElementById("fourmenusubmenu3").style.display = "none";
                document.getElementById("fourmenusubmenu4").style.display = "none";
                document.getElementById("fourmenusubmenu5").style.display = "none";
                document.getElementById("fourmenusubmenu2").style.display = "none";
                document.getElementById("fourmenusubmenu6").style.display = "none";
            }

            if (document.getElementById("wearkit").style.display == "block") {
                document.getElementById("homekit").style.display = "none";
                document.getElementById("supplykit").style.display = "none";
                document.getElementById("jewelrykit").style.display = "none";
                document.getElementById("partykit").style.display = "none";
            }

        }
        
